{
	"name":              "dyndns",
	"version":           "0.1.0",
	"description":       "Dynamic DNS for widschi.",
	"author":            "Jannis R <mail@jannisr.de>",
	"license":           "ISC",
	"homepage":          "https://github.com/derhuerst/dyndns",
	"repository":        "git://github.com/derhuerst/dyndns.git",
	"bugs":              "https://github.com/derhuerst/dyndns/issues",
	"private":           true,
	"main":              "lib/server.js",
	"scripts": {
		"build":             "coffee -cb -o lib src && coffee -cb config",
		"set-password":      "node ./lib/set-password.js",
		"server":            "node ./lib/server.js",
		"start":             "forever start ./lib/server.js",
		"stop":              "forever stop ./lib/server.js",
		"install-service":   "node ./lib/install-service.js",
		"uninstall-service": "node ./lib/uninstall-service.js"
	},
	"dependencies": {
		"prompt-sync":   "^3.0.0",
		"keychain":      "^1.0.0",
		"config":        "^1.19.0",
		"inwx":          "derhuerst/inwx-nodejs",
		"forever":       "^0.15.1",
		"user-startup":  "^0.2.1"
	},
	"devDependencies": {
		"coffee-script": "^1.10.0"
	}
}
